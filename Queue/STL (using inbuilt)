#include<iostream>
#include<queue>
using namespace std;
int main()
{
    
    queue<int> q;
    q.push(1);
    cout<<"front : "<<q.front()<<endl;
     q.push(2);
      q.push(3);
     q.push(4);
     cout<<q.size()<<endl;
     q.pop();
     cout<<q.size()<<endl;
     if(q.empty())
     {
        cout<<"queue is empty"<<endl;
     }
     else{
        cout<<"queue is not empty"<<endl;
     }

    
}
