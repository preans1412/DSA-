#include<iostream>
//#include <bits/stdc++.h>
using namespace std;
int linearSearch(int arr[],int n,int key)
{

    for (int i = 0; i < n; i++)
        if (arr[i] == key)
            return i;
    return -1;
}
int main()
{
    int n;
    cout<<"n- ";
    cin>>n;
    
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int key;
    cout<<"key- ";
    cin>>key;
    int result=  linearSearch(arr,n,key);
    (result == -1)
		? cout << "Element is not present in array"
		: cout << "Element is present at index " << result;
	return 0;
}
